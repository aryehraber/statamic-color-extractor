{
    "name": "aryehraber/statamic-color-extractor",
    "type": "statamic-addon",
    "description": "Extract colors from images.",
    "keywords": [
        "statamic",
        "image color",
        "color extractor"
    ],
    "homepage": "https://github.com/aryehraber/statamic-color-extractor",
    "license": "MIT",
    "require": {
        "jonaskohl/color-extractor": "^0.4",
        "davidgorges/color-contrast": "^1.0",
        "statamic/cms": "^3.3|^4.0"
    },
    "autoload": {
        "psr-4": {
            "AryehRaber\\ColorExtractor\\": "src"
        }
    },
    "authors": [
        {
            "name": "Aryeh Raber",
            "email": "aryeh.raber@gmail.com",
            "homepage": "https://aryeh.dev",
            "role": "Developer"
        }
    ],
    "extra": {
        "statamic": {
            "name": "ColorExtractor",
            "slug": "color_extractor",
            "description": "Extract colors from images."
        },
        "laravel": {
            "providers": [
                "AryehRaber\\ColorExtractor\\ColorExtractorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
